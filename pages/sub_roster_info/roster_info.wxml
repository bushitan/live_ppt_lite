<block wx:for="{{rosterList}}">
    <view class='page__p_15 page_bg_white roster'>
        <xx-navigte   catchclick="click" data-phone="{{item.phone}}">
            <!-- <image slot="icon" src="../../images/share.png"></image>  -->
            <view slot="left" >{{item.name}}</view> 
            <view slot="right" class='phone'>{{item.phone}}</view>
        </xx-navigte>
    </view>
    <view class='page__line_gray'></view>
</block>
